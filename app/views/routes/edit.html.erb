<%= simple_form_for @route do |f| %>
  <%= f.input :starting_system, placeholder: 'Starting System' %>
  <%= f.input :ending_system, placeholder: 'Ending System' %>
  <%= f.input :commodity, placeholder: 'Commodity Purchased' %>
  <%= f.input :buy_price, placeholder: 'Purchase Price' %>
  <%= f.input :sell_price, placeholder: '12.34' %>
  <%= f.input :make_private %>
  <%= f.submit %>
<% end %>
